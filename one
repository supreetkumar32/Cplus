//binary search
int arr[]={1,2,3,4,5,6};
int key=5,ans;
int n=6;
int s=0;
int e=n-1;
int mid=s+(e-s)/2;
while(s<=e){
if(arr[mid]==key){
   ans=mid;
}
else if(key>arr[mid]){
    s=mid+1;
}else{
    e=mid-1;
}
mid=s+(e-s)/2;
}

}
